var weo = KnoemaTest1.UseDS("knoema.com", "vexjcre");
weo.LoadData(weo.Dimensions["Indicator"]["NGDP_R"]);

foreach (var country in weo.Dimensions["Country"])
{
    WriteLine(country["Name"]);
	weo[country, "ngdp_rpch"] = Pch(weo[country, "ngdp_r"]);
}
//weo.SaveData(SaveOptions.Update);
weo.SaveData(SaveOptions.Overwrite);
